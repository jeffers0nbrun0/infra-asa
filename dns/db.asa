;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	asa.dns.br. root.asa.dns.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	s1.prova.asa.br.
@	IN	NS	s2.prova.asa.br.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

asa	IN	A	10.25.2.99 ; apontamento de dominio para ipv4
